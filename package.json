{
  "name": "pdf2scrapbox",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "npm@9.2.0",
  "scripts": {
    "makeJson": "node -r esbuild-register src/makeJson.ts",
    "addOcr": "node -r esbuild-register src/addOcr.ts",
    "test": "jest"
  },
  "prettier": {
    "jsxSingleQuote": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none",
    "arrowParens": "avoid"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/*.test.ts"
    ],
    "moduleNameMapper": {
      "^app/(.+)": "<rootDir>/src/$1"
    }
  },
  "dependencies": {
    "canvas": "^2.11.2",
    "cli-progress": "^3.12.0",
    "fp-ts": "^2.16.0",
    "gyazo-api": "^0.3.1",
    "io-ts": "^2.2.20",
    "monocle-ts": "^2.3.13",
    "newtype-ts": "^0.3.5",
    "pdfjs-dist": "^3.7.107"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/request": "^2.48.8",
    "dotenv": "^16.3.1",
    "esbuild": "^0.18.6",
    "esbuild-register": "^3.4.2",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.3"
  }
}
